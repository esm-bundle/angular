{
  "name": "@esm-bundle/angular",
  "version": "18.2.9",
  "description": "All Angular packages in ESM and SystemJS formats",
  "main": "index.js",
  "module": "esm/index.js",
  "type": "module",
  "scripts": {
    "pretty-quick": "pretty-quick",
    "test": "pnpm run build && concurrently -n w: 'pnpm:test:*'",
    "test:browser": "karma start karma.conf.cjs --single-run",
    "debug:test:browser": "karma start karma.conf.cjs",
    "test:formatting": "prettier --check .",
    "build": "rollup -c",
    "format": "prettier --write .",
    "release": "release-it",
    "prepublishOnly": "pnpm run build && pinst --disable",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "files": [
    "es2022",
    "system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esm-bundle/angular.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/esm-bundle/angular/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/esm-bundle/angular#readme",
  "devDependencies": {
    "@angular/animations": "18.2.9",
    "@angular/common": "18.2.9",
    "@angular/compiler": "18.2.9",
    "@angular/compiler-cli": "18.2.9",
    "@angular/core": "18.2.9",
    "@angular/elements": "18.2.9",
    "@angular/forms": "18.2.9",
    "@angular/localize": "18.2.9",
    "@angular/platform-browser": "18.2.9",
    "@angular/platform-browser-dynamic": "18.2.9",
    "@angular/router": "18.2.9",
    "@angular/service-worker": "18.2.9",
    "@angular/upgrade": "18.2.10",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-replace": "6.0.1",
    "@rollup/plugin-terser": "0.4.4",
    "chai": "5.1.2",
    "concurrently": "9.0.1",
    "husky": "9.1.6",
    "karma": "6.4.4",
    "karma-firefox-launcher": "2.1.3",
    "karma-jasmine": "5.1.0",
    "mocha": "10.8.2",
    "pinst": "3.0.0",
    "prettier": "3.3.3",
    "pretty-quick": "4.0.0",
    "release-it": "17.10.0",
    "release-it-plugin-esm-bundle": "3.0.0",
    "rimraf": "6.0.1",
    "rollup": "4.24.3",
    "rxjs": "7.8.1",
    "tslib": "2.8.0",
    "typescript": "5.6.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "release-it-plugin-esm-bundle@3.0.0": "patches/release-it-plugin-esm-bundle@3.0.0.patch"
    }
  }
}
